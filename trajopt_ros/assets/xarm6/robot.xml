        <motor ctrllimited="true" ctrlrange="-2.0 2.0" forcerange="-2 2" joint="joint6" name="torq_j6"/>
        <position name='pos_left_finger' ctrllimited="true" kp="10" joint='drive_joint' ctrlrange='0 0.85'/>
        <position name='pos_right_finger' ctrllimited="true" kp="10" joint='right_outer_knuckle_joint' ctrlrange='0 0.85'/>
